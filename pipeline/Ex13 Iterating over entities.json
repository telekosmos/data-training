{
	"name": "Ex13 Iterating over entities",
	"properties": {
		"description": "Call pipeline to move parquet files by entities by iterating over parameter",
		"activities": [
			{
				"name": "LoopOverEntities",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
