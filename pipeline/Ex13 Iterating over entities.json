							"userProperties": [],
							"typeProperties": {
								"pipeline": {
									"referenceName": "Move parquet files by entity",
									"type": "PipelineReference"
								},
								"waitOnCompletion": true,
								"parameters": {
									"entity": {
										"value": "@item()",
										"type": "Expression"
									}
								}
							}
						}
					]
				}
			}
		],
		"parameters": {
			"entities": {
				"type": "string",
				"defaultValue": "Events,Product,Traffic"
			}
		},
